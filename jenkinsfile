pipeline {
    agent any

    stages {
      
        stage('Test') {
            steps {
                sh '''
                python api.py
                '''
            }
        }
    }

    post {
        always {
            echo 'Pipeline finished.'
        }
        success {
            echo 'Pipeline successful!'
        }
        failure {
            echo 'Pipeline failed!'
        }
    }
}
